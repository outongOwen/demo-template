{
  "name": "editor-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run typecheck && cross-env VITE_SERVICE_ENV=prod vite build",
    "build:dev": "npm run typecheck && cross-env VITE_SERVICE_ENV=dev vite build",
    "build:test": "npm run typecheck && cross-env VITE_SERVICE_ENV=test vite build",
    "build:vercel": "cross-env VITE_HASH_ROUTE=Y VITE_VERCEL=Y vite build",
    "clear": "rimraf pnpm-lock.yaml node_modules ",
    "create-template": "plop --plopfile ./plop/plopfile.js",
    "dev": "cross-env VITE_SERVICE_ENV=dev vite",
    "dev:prod": "cross-env VITE_SERVICE_ENV=prod vite",
    "dev:test": "cross-env VITE_SERVICE_ENV=test vite",
    "init-git": "npx simple-git-hooks",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:lint-staged": "lint-staged",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "ncu --deep -u"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{scss,less,style,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@interactjs/types": "^1.10.18",
    "@vueuse/core": "^10.4.1",
    "axios": "1.5.0",
    "better-mock": "^0.3.6",
    "colord": "^2.9.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.9",
    "fabric": "6.0.0-beta12",
    "interactjs": "^1.10.18",
    "lodash": "^4.17.21",
    "naive-ui": "2.34.4",
    "pinia": "^2.1.6",
    "qs": "^6.11.2",
    "splitpanes": "^3.1.5",
    "type-fest": "^4.3.1",
    "uuid": "^9.0.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "xgplayer": "^3.0.9",
    "xgplayer-mp4": "^3.0.9",
    "xgplayer-music": "^3.0.9"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.8",
    "@iconify/json": "^2.2.114",
    "@iconify/vue": "^4.1.1",
    "@types/crypto-js": "^4.1.2",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "20.5.7",
    "@types/qs": "^6.9.8",
    "@types/splitpanes": "^2.2.1",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@unocss/preset-attributify": "^0.55.7",
    "@unocss/preset-uno": "^0.55.7",
    "@unocss/transformer-directives": "^0.55.7",
    "@unocss/vite": "^0.55.7",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "compressing": "^1.10.0",
    "consola": "^3.2.3",
    "conventional-changelog": "^5.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-soybeanjs": "^0.5.6",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "esno": "^0.17.0",
    "lint-staged": "^14.0.1",
    "npm-check-updates": "^16.14.0",
    "ocr-tool": "git+ssh://git@gitlab.cmvideo.cn:7999/production_capacity/clip/clipcloud-ocr.git#pubTs",
    "plop": "^3.1.2",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.5",
