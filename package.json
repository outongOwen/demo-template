  },
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "typescript": ">=5.0.0"
  },
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">=16.20.0",
    "pnpm": ">=8.1.0"
  }
}
