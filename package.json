{
  "name": "editor-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run typecheck && cross-env VITE_SERVICE_ENV=prod vite build",
    "build:dev": "npm run typecheck && cross-env VITE_SERVICE_ENV=dev vite build",
    "build:test": "npm run typecheck && cross-env VITE_SERVICE_ENV=test vite build",
    "build:vercel": "cross-env VITE_HASH_ROUTE=Y VITE_VERCEL=Y vite build",
    "clear": "rimraf pnpm-lock.yaml node_modules ",
    "create-temp": "plop --plopfile ./plop/plopfile.js",
    "dev": "cross-env VITE_SERVICE_ENV=dev vite",
    "dev:prod": "cross-env VITE_SERVICE_ENV=prod vite",
    "dev:test": "cross-env VITE_SERVICE_ENV=test vite",
    "init-git": "npx simple-git-hooks",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:lint-staged": "lint-staged",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "ncu --deep -u"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{scss,less,style,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.1",
    "@vueuse/core": "^10.1.2",
    "axios": "1.4.0",
    "better-mock": "^0.3.6",
    "colord": "^2.9.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "lodash-es": "^4.17.21",
    "naive-ui": "2.34.3",
    "pinia": "^2.1.1",
    "qs": "^6.11.2",
    "splitpanes": "^3.1.5",
    "type-fest": "^3.10.0",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.3",
    "@iconify/json": "^2.2.66",
    "@iconify/vue": "^4.1.1",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "20.2.0",
    "@types/qs": "^6.9.7",
    "@types/splitpanes": "^2.2.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@unocss/preset-attributify": "^0.51.13",
    "@unocss/preset-uno": "^0.51.13",
    "@unocss/transformer-directives": "^0.51.13",
    "@unocss/vite": "^0.51.13",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "compressing": "^1.9.0",
    "consola": "^3.1.0",
    "conventional-changelog": "^3.1.25",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-soybeanjs": "^0.3.7",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.13.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.2",
    "npm-check-updates": "^16.10.12",
    "plop": "^3.1.2",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.3",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "simple-git-hooks": "^2.8.1",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "unocss-preset-scrollbar": "^0.2.1",
    "unplugin-auto-import": "^0.16.0",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "0.24.1",
    "unplugin-vue-define-options": "^1.3.5",
    "vite": "^4.3.7",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "2.9.8",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.6.5"
  },
  "peerDependencies": {
    "eslint": ">=7.4.0",
    "typescript": ">=4.8.0"
  },
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">=16.20.0",
    "pnpm": ">=8.1.0"
  }
}
